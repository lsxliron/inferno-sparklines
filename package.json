{
  "name": "react-sparklines",
  "version": "1.7.0",
  "description": "Beautiful and expressive Sparklines component for React ",
  "main": "build/index.js",
  "directories": {
    "src": "src/"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --mode development --progress",
    "test": "NODE_ENV=test jest --testPathIgnorePatterns __tests__/helpers",
    "test:watch": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=production webpack -p --config webpack.config.prod.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<root_dir>/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsxliron/inferno-sparklines.git"
  },
  "keywords": [
    "inferno",
    "component",
    "inferno-component",
    "charts",
    "sparklines",
    "visualization",
    "jsx"
  ],
  "author": "Boris Yankov <borisyankov@gmail.com> (https://github.com/borisyankov)",
  "contributors": [
    {
      "name": "Liron Shimrony",
      "email": "lsxliron@gmail.com",
      "url": "https://github.com/lsxliron"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsxliron/inferno-sparklines/issues"
  },
  "homepage": "https://github.com/lsxliron/inferno-sparklines#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "babel-cli": "^6.23.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-inferno": "^6.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-inferno-app": "^8.0.3",
    "chai": "^4.1.0",
    "clean-webpack-plugin": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-inferno": "^1.3.1",
    "html-differ": "^1.4.0",
    "html-webpack-plugin": "^3.2.0",
    "inferno": "^7.1.7",
    "inferno-create-element": "^7.1.9",
    "inferno-dev-utils": "^6.0.4",
    "inferno-server": "^7.1.9",
    "inferno-vnode-flags": "^7.1.8",
    "jest": "^24.1.0",
    "path": "^0.12.7",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
